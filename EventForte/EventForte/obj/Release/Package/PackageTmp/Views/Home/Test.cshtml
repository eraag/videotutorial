@model IEnumerable<EventForte.Models.EventDataContext>
@{
    ViewBag.Title = "Dashboard";
    Layout = null;
}

<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="utf-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width" />
    <title>Dashboard</title>
    <!-- Bootstrap -->
    <link href="~/Content/css/bootstrap.min.css" rel="stylesheet">
    <link href="~/Content/css/font-awesome.min.css" rel="stylesheet">
    <link href="~/Content/css/jquery-ui.css" rel="stylesheet">

    <!-- Custom css -->
    <link href="~/Content/css/main-style.css" rel="stylesheet">
    <link rel="stylesheet" href="~/Content/css/build.css" />

</head>
<body>

    <header>
        <nav class="navbar navbar-default navbar-static-top">
            <div class="container-header row">
                <a class="pull-left logo" href="#"> <img src="~/Content/images/event-forte-logo.png" alt="Event Fort"> </a>
                <ul class="pull-left">
                    <li class="events active"><a href="#">Events</a></li>
                    <li class="activity"><a href="#">Activities</a><span class="green-dot"></span></li>
                    <li class="team"><a href="#">Team</a></li>
                </ul>
                <ul class="pull-right nav-right">
                    <li class="upgrade-now">
                        <i class="glyphicon glyphicon-upload"></i>
                        <div>
                            <div class="text-nor">YOU HAVE 7 DAYS LEFT</div>
                            <div class="text-upgrade">UPGRADE NOW!</div>
                        </div>
                    </li>
                    <li>
                        <span class="circle">JH</span>
                        <div class="txt-name border-right">
                            <span> Johannes H.</span>
                            <i class="glyphicon glyphicon-menu-down"></i>
                        </div>
                    </li>
                    <li class="dots"><a href="#"></a></li>
                    <li class="bell pos-rel"><a href="#"><span class="circle-notice">2</span></a></li>
                    <li class="question"><a href="#"></a></li>

                </ul>
            </div>
        </nav>
    </header>

    <div class="container red-section">
        <div class="red-panel row">
            <div class="col-md-11"><i class="glyphicon glyphicon-info-sign"></i> <p>EventForte lets you build custom rooms. Add an event and invite team members collaboratively design room layouts. <img src="~/Content/images/learn.png" alt="" /> <span class="tips">Learn Tips</span></p></div>
            <div class="col-md-1 noPaddingRight"><i class="glyphicon glyphicon-remove pull-right" id="close-panel"></i> </div>
        </div>
    </div>

    <!-- top container white -->
    <div class="container white-section ">
        <div class="white-panel row">
            <nav class="navbar navbar-default navbar-static-top mar0">
                <div class="container-header row">
                    <ul class="pull-left mevent">
                        <li class="dropdown">
                            <a class="dropdown-toggle border-right" data-toggle="dropdown" href="#">My Events <i class="glyphicon glyphicon-menu-down"></i></a>
                            <ul class="dropdown-menu">
                                <li class="green"><a href="#">Organizing</a></li>
                                <li><a href="#">Attendin</a></li>
                                <li><a href="#">Invited</a></li>
                            </ul>
                        </li>

                        <li>
                            <div class="txt-name">
                                <button type="button" class="btn btn-danger">New event</button>
                            </div>
                        </li>
                    </ul>
                    <ul class="pull-right nav-right">
                        <li class="date-control">
                            <div class="txt-name">
                                <input type="text" id="from" name="from" placeholder="Feb 15, 2015">
                                <label for="to"><i class="glyphicon glyphicon-arrow-right"></i></label>
                                <input type="text" id="to" name="to" placeholder="Mar 15, 2015">
                            </div>
                        </li>

                        <li class="dropdown">

                            @*@Html.DropDownListFor(m => m.First().catid, new SelectList(ViewBag.Category as System.Collections.IEnumerable,
                                "Id", "CategoryName"), "Category", new { id = "catId" })*@
                            @Html.DropDownListFor(m => m.First().catid, new SelectList(ViewBag.EventType as System.Collections.IEnumerable,
                                       "TypeId", "TypeName"), "Category", new { id = "TypeId" })

                        </li>
                    </ul>

                </div>
            </nav>
        </div>
    </div>



    <!-- top container grey -->

    <div class="container grey-section">
        <div class="grey-panel row">
            <div class="col-md-6 grey-left">
                <div class="checkbox checkbox-primary pull-left">
                    <input class="styled" id="main-select-all" type="checkbox">
                    <label></label>
                </div>
                <div class="btn-group" role="group" aria-label="...">
                    <button type="button" class="btn btn-default"><img src="~/Content/images/icon1.png" alt="" /></button>
                    <button type="button" class="btn btn-default"><img src="~/Content/images/icon2.png" alt="" /></button>
                    <button type="button" class="btn btn-default" id="btndelete"><img src="~/Content/images/delete.png" alt="" /></button>
                </div>
            </div>
            <div class="col-md-6 grey-right noPaddingRight">
                <ul class="pull-right">
                    <li> 4 events sorted by <a href="#">category</a> <span class="plus">+</span><a href="#"> location</a> </li>
                    <li class="search pos-rel" id="search-item">  <span class="icon">Search</span><input type="text" maxlength="40" placeholder="Search Event" class="search-item"></li>
                </ul>
            </div>
        </div>
    </div>

    <div class="container main-section">
        <div class="container-main row">
            <ul class="organized-events col-md-12">
                <li class="dropdown">
                    <a class="dropdown-toggle my-events" data-toggle="dropdown" href="#">ORGANIZED EVENTS <i class="glyphicon glyphicon-menu-down"></i></a>
                    <ul class="dropdown-menu">
                        <li class="green"><a href="#">Organizing (10)</a></li>
                        <li><a href="#">Attending (5)</a></li>
                        <li><a href="#">Invited (1)</a></li>
                    </ul>
                </li>
                <li>
                    <div class="checkbox checkbox-primary pull-left">
                        <input class="styled chkbtn" type="checkbox">
                        <label>Archived</label>
                    </div>
                </li>
                <li>
                    <div class="checkbox checkbox-primary pull-left">
                        <input class="styled chkbtn" type="checkbox">
                        <label>Completed</label>
                    </div>
                </li>
            </ul>

            <div class="col-md-1 scrollspy">
                <nav id="target_nav" data-spy="affix">
                    <ul class="nav">
                        @*<li class="active"><a href="#2015">2015</a></li>
                            <li><a href="#2014">2014</a></li>
                            <li><a href="#2013">2013</a></li>
                            <li><a href="#2012">2012</a></li>*@
                        @*@foreach (var year in @ViewBag.varyear)
                            {

                                <li class="active"><a href="#@Html.Encode(year.memdate)">@Html.Encode(year.memdate)</a></li>

                            }*@

                        @for (int i = 0; i < ViewBag.varyear.Count; i++)
                        {
                            if (i == 0)
                            {
                                <li class="active"><a href="#@Html.Encode(@ViewBag.varyear[i].memdate)" class="year">@Html.Encode(@ViewBag.varyear[i].memdate)</a></li>
                            }
                            else
                            {
                                <li><a href="#@Html.Encode(@ViewBag.varyear[i].memdate)" class="year">@Html.Encode(@ViewBag.varyear[i].memdate)</a></li>
                            }
                        }

                    </ul>
                </nav>
            </div>
            <div id="myScrollspy" class="col-md-11">
                <!-- year 2015 -->
                <section id="2015">
                    <div id="divJan">
                        <ul class="thumbnails january" id="hover-cap-4col">
                            <li class="month">
                                <div class="checkbox checkbox-primary pull-left">
                                    <input type="checkbox" id="jan15" class="styled select-all">
                                    <label></label>
                                    <span class="month-text">JAN</span>
                                </div>

                            </li>


                            @foreach (var urladdress in @ViewBag.EventVarDataJan)
                            {
                                <li class="col-md-4">
                                    <div class="checkbox checkbox-primary pull-left">
                                        <input type="checkbox" name="jan14" value=@urladdress.EventId class="styled jan15">
                                        <label></label>
                                    </div>
                                    <div class="thumbnail">
                                        <div class="caption">
                                            <div class="icon">
                                                <a href="#"><span><img src="~/Content/images/cap-icon1.png" /></span></a>
                                                <a href="#"><span><img src="~/Content/images/cap-icon2.png" /></span></a>
                                                <a href="#"><span><img src="~/Content/images/cap-icon3.png" /></span></a>
                                                <a href="#"><span><img src="~/Content/images/cap-icon4.png" /></span></a>
                                            </div>

                                        </div>

                                        <img src="@Url.Content(urladdress.ImageURL)" />
                                        <h5> @Html.Encode(urladdress.Name)</h5>
                                        <p> @Html.Encode(urladdress.Description)</p>
                                        <p class="seperator"></p>
                                        <h6>Event Location @Html.Encode(urladdress.LocationName)</h6>
                                        <h6>Event Date @Html.Encode(urladdress.FromDate.ToString("MM/dd/yyyy")) to @Html.Encode(urladdress.ToDate.ToString("MM/dd/yyyy"))</h6>
                                        <h6>Last Updated @Html.Encode(urladdress.LastUpdated)</h6>
                                    </div>
                                </li>
                            }


