
<div id="myScrollspy" class="col-md-11">
    <!-- year 2015 -->
    <section id="2015">


        <div id="divDec">
            <ul class="thumbnails  nov" id="hover-cap-4col">
                <li class="month">
                    <div class="checkbox checkbox-primary pull-left">
                        <input type="checkbox" class="styled select-all" id="dec15">
                        <label></label>
                        <span class="month-text">DEC</span>
                    </div>

                </li>
                @foreach (var urladdress in @ViewBag.EventVarDataDec)
                {
                    <li class="col-md-4">
                        <div class="checkbox checkbox-primary pull-left">
                            <input type="checkbox" name="dec14" value=@urladdress.EventId class="styled dec15">
                            <label></label>
                        </div>
                        <div class="thumbnail">
                            <div class="caption">
                                <div class="icon">
                                    <a href="#"><span><img src="~/Content/images/cap-icon1.png" /></span></a>
                                    <a href="#"><span><img src="~/Content/images/cap-icon2.png" /></span></a>
                                    <a href="#"><span><img src="~/Content/images/cap-icon3.png" /></span></a>
                                    <a href="#"><span><img src="~/Content/images/cap-icon4.png" /></span></a>
                                </div>
                                <h4>@Html.Encode(urladdress.Name)</h4>
                                <p>@Html.Encode(urladdress.LocationName)</p>
                                <p>@Html.Encode(urladdress.FromDate.ToString("MM/dd/yyyy")) to @Html.Encode(urladdress.ToDate.ToString("MM/dd/yyyy"))</p>
                                <p><button class="btn btn-default">SHOW DETAILS</button></p>
                                <div class="cap-bottom">
                                    @foreach (var collab in @ViewBag.EventCollaborator)
                                    {
                                        if (@collab.eventid == @urladdress.EventId)
                                        {
                                            <span class="circle">@Html.Encode(collab.initial)</span>
                                            
                                        }
                                    }
                                </div>
                            </div>

                            <img src="@Url.Content(urladdress.ImageURL)" />
                            <h5> @Html.Encode(urladdress.Name)</h5>
                            <p> @Html.Encode(urladdress.Description)</p>
                            <p class="seperator"></p>
                            <h6>Event Location @Html.Encode(urladdress.LocationName)</h6>
                            <h6>Event Date @Html.Encode(urladdress.FromDate.ToString("MM/dd/yyyy")) to @Html.Encode(urladdress.ToDate.ToString("MM/dd/yyyy"))</h6>
                            <h6>Last Updated @Html.Encode(urladdress.LastUpdated)</h6>
                        </div>
                    </li>
                }

            </ul>

        </div>


